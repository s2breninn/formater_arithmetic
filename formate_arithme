def arithmetic_arranger(expressions):
    list_expression = []

    for expression in expressions:
        converted_results = []
        for caracter in expression.split():
            if caracter.isdigit():
                caracter = int(caracter)
            converted_results.append(caracter)
        list_expression.append(converted_results)
    return list_expression

def calculate_express(list_expression):
    list_op = []
    for expression in list_expression:
        nums = []
        op = ''

        for algoritmo in expression:
            if algoritmo not in ['+', '-']:
                nums.append(algoritmo)
                if len(nums) == 2:
                    if op == '+':
                        result = nums[0] + nums[1]
                        expression.append(result)
                        nums = []
                    if op == '-':
                        result = nums[0] - nums[1]
                        expression.append(result)
                        nums = []
            else:
                op = algoritmo
                list_op.append(op)

    return list_expression, list_op

def print_exp(calculate_express, list_op):
    for val in calculate_express:
        for algoritmo in val:
            u = algoritmo.find('+')
            print(u)


lista = arithmetic_arranger(["32 + 698", "3801 - 2", "45 + 43", "123 + 49"])
calculate_express, list_op = calculate_express(lista)
print_exp(calculate_express, list_op)
print(calculate_express, list_op)
